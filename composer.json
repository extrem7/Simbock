{
    "name": "extrem7/simbok",
    "type": "project",
    "description": "The Simbok project.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "artesaos/seotools": "^0.18.0",
        "coderello/laravel-shared-data": "^2.0",
        "cviebrock/eloquent-sluggable": "^7.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "devmarketer/easynav": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.24",
        "laravel/socialite": "^5.0",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.2",
        "nwidart/laravel-modules": "^7.1",
        "propaganistas/laravel-phone": "^4.2",
        "spatie/eloquent-sortable": "^3.8",
        "spatie/laravel-medialibrary": "^8.7.0",
        "spatie/laravel-permission": "^3.13",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-eager-limit": "^1.5",
        "tightenco/ziggy": "^0.9.3",
        "zschuessler/laravel-route-to-class": "^2.9"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "barryvdh/laravel-debugbar": "^3.3.3",
        "barryvdh/laravel-ide-helper": "^2.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/functions.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        },
        "classmap": [
            "database/seeds",
            "database/seeds/Jobs",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    }
}
